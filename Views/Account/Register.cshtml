<style>
    .radio_check {
        display: flex;
        width: 100%;
        align-items: center;
        gap: 10px;
    }

    .radio_check input {
        width: 15px;
    }

    .radio-group div {
        gap: 10px;
    }

    .form-check {
        margin-bottom: 0.5rem; /* Space between checkboxes */
    }

    .form-check-input {
        width: 1.25rem; /* Smaller width for checkboxes */
        height: 1.25rem; /* Smaller height for checkboxes */
    }

    .form-label {
        font-weight: bold; /* Bold label */
        margin-bottom: 0.5rem; /* Space between label and checkboxes */
    }

    .paypal-options {
    display: flex;
    flex-direction: column; /* Align options vertically */
}

.paypal-option {
    position: relative; /* Position for custom styling */
    padding-left: 30px; /* Space for the custom radio button */
    margin: 10px 0; /* Spacing between options */
    cursor: pointer; /* Pointer on hover */
    font-size: 16px; /* Modern font size */
}

.paypal-option input[type="radio"] {
    position: absolute; /* Hide default radio */
    opacity: 0; /* Make it invisible */
}

.paypal-option input[type="radio"] + .option-label {
    position: relative;
    padding-left: 30px; /* Space for custom radio */
}

.paypal-option input[type="radio"] + .option-label::before {
    content: '';
    position: absolute;
    left: 0; /* Align to left */
    top: 50%; /* Center vertically */
    transform: translateY(-50%); /* Center it */
    width: 18px; /* Custom radio size */
    height: 18px; /* Custom radio size */
    border: 2px solid #007bff; /* Border color */
    border-radius: 50%; /* Round shape */
    background-color: #fff; /* Background color */
    transition: background-color 0.3s; /* Smooth transition */
}

.paypal-option input[type="radio"]:checked + .option-label::before {
    background-color: #007bff; /* Checked background color */
    border-color: #0056b3; /* Checked border color */
}

.text-danger {
    display: block;
    margin-top: 5px; /* Space between radio and validation message */
}

/* Styles for the webcam and feedback radio buttons */
.webcam-options,
.feedback-options {
    display: flex;
    flex-direction: column; /* Align options vertically */
}

.webcam-option,
.feedback-option {
    position: relative; /* Position for custom styling */
    padding-left: 30px; /* Space for the custom radio button */
    margin: 10px 0; /* Spacing between options */
    cursor: pointer; /* Pointer on hover */
    font-size: 16px; /* Modern font size */
}

.webcam-option input[type="radio"],
.feedback-option input[type="radio"] {
    position: absolute; /* Hide default radio */
    opacity: 0; /* Make it invisible */
}

.webcam-option input[type="radio"] + .option-label,
.feedback-option input[type="radio"] + .option-label {
    position: relative;
    padding-left: 30px; /* Space for custom radio */
}

.webcam-option input[type="radio"] + .option-label::before,
.feedback-option input[type="radio"] + .option-label::before {
    content: '';
    position: absolute;
    left: 0; /* Align to left */
    top: 50%; /* Center vertically */
    transform: translateY(-50%); /* Center it */
    width: 18px; /* Custom radio size */
    height: 18px; /* Custom radio size */
    border: 2px solid #007bff; /* Border color */
    border-radius: 50%; /* Round shape */
    background-color: #fff; /* Background color */
