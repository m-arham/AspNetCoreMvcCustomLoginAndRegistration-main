}

.webcam-option input[type="radio"]:checked + .option-label::before,
.feedback-option input[type="radio"]:checked + .option-label::before {
    background-color: #007bff; /* Checked background color */
    border-color: #0056b3; /* Checked border color */
}

.text-danger {
    display: block;
    margin-top: 5px; /* Space between radio and validation message */
}


</style>


