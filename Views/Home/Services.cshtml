    <script>
        $(document).ready(function() {
            $('.read-more-btn').on('click', function() {
                const card = $(this).parent();
                const moreText = card.find('.more-text');
                if (moreText.is(':visible')) {
                    moreText.hide();
                    $(this).text('Read More');
                } else {
                    moreText.show();
                    $(this).text('Read Less');
                }
            });
        });
    </script>